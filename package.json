{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "start": "rimraf dist && run-p watch:*",
    "clean": "rimraf dist",
    "check": "svelte-check",
    "build": "run-s clean check compile",
    "compile": "cross-env NODE_ENV=production rollup --config",
    "watch:check": "svelte-check --watch",
    "watch:build": "cross-env NODE_ENV=development rollup --config --watch"
},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-html2": "^2.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.22",
    "svelte-preprocess": "^4.1.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "rimraf": "^3.0.2",
    "sirv-cli": "^1.0.0"
  }
}
